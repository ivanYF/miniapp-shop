{
    "pages": [
        "pages/index/index",
        "pages/core/core",
        "pages/my/my",
        "pages/detail/detail"
    ],
    "window": {
        // 类型：HexColor；默认值：#000000
        // 导航栏背景颜色，如"#000000"
        "navigationBarBackgroundColor": "#ffffff",
        // 类型：String；默认值：white
        // 导航栏标题颜色，仅支持 black/white
        "navigationBarTextStyle": "black",
        // 类型：String；默认值：无
        // 导航栏标题文字内容
        "navigationBarTitleText": "WeApp Boilerplate",
        // 类型：HexColor；默认值：#ffffff
        // 窗口的背景色
        "backgroundColor": "#000000",
        // 类型：String；默认值：dark
        // 下拉背景字体、loading 图的样式，仅支持 dark/light
        "backgroundTextStyle": "light",
        // 类型：Boolean；默认值：false
        // 是否开启下拉刷新，详见页面相关事件处理函数。
        "enablePullDownRefresh": false
    },
    "tabBar": {
        "color": "#999999",
        "selectedColor": "#333333",
        "borderStyle": "white",
        "backgroundColor": "#f5f5f5",
        "list": [
        {
            "text": "首页",
            "pagePath": "pages/index/index",
            "iconPath": "images/icon_home.png",
            "selectedIconPath": "images/icon_home_fill.png"
        },
        {
            "text": "商城",
            "pagePath": "pages/core/core",
            "iconPath": "images/icon_shop.png",
            "selectedIconPath": "images/icon_shop_fill.png"
        },
        {
            "text": "我的",
            "pagePath": "pages/my/my",
            "iconPath": "images/icon_my.png",
            "selectedIconPath": "images/icon_my_fill.png"
        }
        ]
    },
    "networkTimeout": {
        "request": 10000,
        "downloadFile": 10000
    },
    "debug": true
}
